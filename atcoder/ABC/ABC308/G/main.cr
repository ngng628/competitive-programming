q = int

mset = NgLib::AATreeMultiset(Int64).new
mins = NgLib::AATreeMultiset(Int64).new
mn = OO
q.times do
  query = ints
  case query[0]
  when 1
    x = query[1]
    mset << x
    i = mset.less_equal_index(x).not_nil!
    if mset[i + 1]? == x && mset[i + 2]? == x
      mins << 0_i64
      mins << 0_i64
    else
      mins << (x ^ mset.at(i - 1)) if i >= 1
      mins << (x ^ mset.at(i + 1)) if i < mset.size - 1
      mins.delete(mset.at(i + 1) ^ mset.at(i - 1)) if i >= 1 && i < mset.size - 1
    end
  when 2
    x = query[1]
    i = mset.less_equal_index(x).not_nil!
    mins.delete(x ^ mset.at(i + 1)) if i < mset.size - 1
    mins.delete(x ^ mset.at(i - 1)) if i >= 1
    mins << (mset.at(i + 1) ^ mset.at(i - 1)) if i >= 1 && i < mset.size - 1
    mset.delete(x)
  when 3
    puts mins.first
  end
end
